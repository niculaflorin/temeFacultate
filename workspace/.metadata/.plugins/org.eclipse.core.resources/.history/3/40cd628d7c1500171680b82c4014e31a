package mds.servicii.clientsample;

import mds.servicii.*;

public class ClientSample {

	public static void main(String[] args) {
	        System.out.println("***********************");
	        System.out.println("Create Web Service Client...");
	        Mecanica service1 = new Mecanica();
	        System.out.println("Create Web Service...");
	        MecanicaPortType port1 = service1.getMecanicaHttpSoap11Endpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port1.getLucruMecanic(Double.parseDouble("3.0"),Double.parseDouble("4.0"),Double.parseDouble("0")));
	        System.out.println("Server said: " + port1.getFortaElastica(Double.parseDouble(args[3]),Double.parseDouble(args[4])));
	        System.out.println("Server said: " + port1.getForta(Double.parseDouble(args[5]),Double.parseDouble(args[6])));
	        System.out.println("Server said: " + port1.getFortaFrecare(Double.parseDouble(args[7]),Double.parseDouble(args[8]),Double.parseDouble(args[9])));
	        System.out.println("Create Web Service...");
	        MecanicaPortType port2 = service1.getMecanicaHttpSoap11Endpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port2.getLucruMecanic(Double.parseDouble(args[10]),Double.parseDouble(args[11]),Double.parseDouble(args[12])));
	        System.out.println("Server said: " + port2.getFortaElastica(Double.parseDouble(args[13]),Double.parseDouble(args[14])));
	        System.out.println("Server said: " + port2.getForta(Double.parseDouble(args[15]),Double.parseDouble(args[16])));
	        System.out.println("Server said: " + port2.getFortaFrecare(Double.parseDouble(args[17]),Double.parseDouble(args[18]),Double.parseDouble(args[19])));
	        System.out.println("Create Web Service...");
	        MecanicaPortType port3 = service1.getMecanicaHttpEndpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port3.getLucruMecanic(Double.parseDouble(args[20]),Double.parseDouble(args[21]),Double.parseDouble(args[22])));
	        System.out.println("Server said: " + port3.getFortaElastica(Double.parseDouble(args[23]),Double.parseDouble(args[24])));
	        System.out.println("Server said: " + port3.getForta(Double.parseDouble(args[25]),Double.parseDouble(args[26])));
	        System.out.println("Server said: " + port3.getFortaFrecare(Double.parseDouble(args[27]),Double.parseDouble(args[28]),Double.parseDouble(args[29])));
	        System.out.println("Create Web Service...");
	        MecanicaPortType port4 = service1.getMecanicaHttpEndpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port4.getLucruMecanic(Double.parseDouble(args[30]),Double.parseDouble(args[31]),Double.parseDouble(args[32])));
	        System.out.println("Server said: " + port4.getFortaElastica(Double.parseDouble(args[33]),Double.parseDouble(args[34])));
	        System.out.println("Server said: " + port4.getForta(Double.parseDouble(args[35]),Double.parseDouble(args[36])));
	        System.out.println("Server said: " + port4.getFortaFrecare(Double.parseDouble(args[37]),Double.parseDouble(args[38]),Double.parseDouble(args[39])));
	        System.out.println("Create Web Service...");
	        MecanicaPortType port5 = service1.getMecanicaHttpSoap12Endpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port5.getLucruMecanic(Double.parseDouble(args[40]),Double.parseDouble(args[41]),Double.parseDouble(args[42])));
	        System.out.println("Server said: " + port5.getFortaElastica(Double.parseDouble(args[43]),Double.parseDouble(args[44])));
	        System.out.println("Server said: " + port5.getForta(Double.parseDouble(args[45]),Double.parseDouble(args[46])));
	        System.out.println("Server said: " + port5.getFortaFrecare(Double.parseDouble(args[47]),Double.parseDouble(args[48]),Double.parseDouble(args[49])));
	        System.out.println("Create Web Service...");
	        MecanicaPortType port6 = service1.getMecanicaHttpSoap12Endpoint();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port6.getLucruMecanic(Double.parseDouble(args[50]),Double.parseDouble(args[51]),Double.parseDouble(args[52])));
	        System.out.println("Server said: " + port6.getFortaElastica(Double.parseDouble(args[53]),Double.parseDouble(args[54])));
	        System.out.println("Server said: " + port6.getForta(Double.parseDouble(args[55]),Double.parseDouble(args[56])));
	        System.out.println("Server said: " + port6.getFortaFrecare(Double.parseDouble(args[57]),Double.parseDouble(args[58]),Double.parseDouble(args[59])));
	        System.out.println("***********************");
	        System.out.println("Call Over!");
	}
}
